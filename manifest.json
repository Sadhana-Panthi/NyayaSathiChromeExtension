{
  "manifest_version": 3,
  "name": "Women's Rights AI Assistant",
  "version": "1.0",
  "description": "An AI-powered assistant for women's rights information using Chrome's built-in AI",
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}